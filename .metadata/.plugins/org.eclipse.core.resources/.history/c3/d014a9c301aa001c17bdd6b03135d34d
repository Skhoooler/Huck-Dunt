/******************************************************
 * Huck Dunt Controller
 *
 * Receives input from a potentiometer and the onboard
 * switches, and sends that information to the console
 * microcontroller
 */

#include "msp.h"


/**
 * main.c
 */
void main(void)
{
	WDT_A->CTL = WDT_A_CTL_PW | WDT_A_CTL_HOLD;		// stop watchdog timer
}
